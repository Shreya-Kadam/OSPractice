#Dockerfile
FROM openjdk:latest
WORKDIR /app
COPY . /app
RUN javac Add.java
CMD ["java","Add"]




vi file
vi dockerfile
docker build -t dockerfile .   #dockerfile name should be same as previous cmds
docker images   #check buid is succeful or not
docker run -it dockerfile



